<%- include('./partials/chore.ejs') -%>
<link rel="stylesheet" href="stylesheets/sign-in.css">
<div class="customsignin">
<div class="middle">
<form class="form-signin" method="post" action="/student-login">
    <h1 class="font-weight-normal mb-3 text-center">Sign in</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" class="form-control mt-2" placeholder="Email address" name="usremail" required autofocus>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" class="form-control mt-2" placeholder="Password" name="usrpsw" required>
    <button class="btn btn-size btn-block mt-2" type="submit">Sign in</button>
  </form>
</div>
</div>
</body>
